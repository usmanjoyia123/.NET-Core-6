
@model WebApp.Models.Make

@{
    ViewData["Title"] = "New Make";
}

<h4>Add a new Make</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TimeStamp" class="control-label"></label>
                <input asp-for="TimeStamp" class="form-control" />
                <span asp-validation-for="TimeStamp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input style="margin-top:20px" type="submit" value="Create" class="btn btn-outline-primary" />

                <a asp-action="Index" style="margin-top:20px" class="btn btn-outline-primary">Back to List</a>
            </div>
        </form>
    </div>
</div>

<div>
  
</div>

